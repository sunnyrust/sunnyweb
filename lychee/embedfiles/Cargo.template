[dependencies]
askama = "0.14"
axum = "0.6"
axum-extra = { version = "0.4.2", features = ["spa"] }
config = "0.13"
dotenv = "0.15"
serde = { version="1", features = ["derive"] }
serde_json = "1"
tera = "1.20.0"
tokio = { version = "1.25", features = ["full"] }
tower-http = { version = "0.3.5", features = ["fs", "trace"] }
tracing = "0.1.37"
tracing-subscriber = { version = "0.3", features = ["env-filter"] }
lazy_static = "1.5.0"